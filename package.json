{
	"name": "@enuesaa/pointmap",
	"version": "0.0.2",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "rm -rf .svelte-kit && svelte-kit sync && vite dev --port 3000",
		"build": "vite build",
		"format": "prettier --write ."
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "^3.0.5",
		"@sveltejs/kit": "^2.6.1",
		"@sveltejs/vite-plugin-svelte": "4.0.0-next.7",
		"@types/node": "^22.7.4",
		"autoprefixer": "^10.4.20",
		"postcss": "^8.4.47",
		"prettier-plugin-svelte": "^3.2.7",
		"svelte": "5.0.0-next.246",
		"tailwindcss": "^3.4.13",
		"typescript": "^5.6.2",
		"vite": "^5.4.8"
	},
	"dependencies": {
		"bits-ui": "^0.21.15",
		"clsx": "^2.1.1",
		"tailwind-merge": "^2.5.2",
		"tailwind-variants": "^0.2.1"
	},
	"prettier": {
		"printWidth": 120,
		"tabWidth": 2,
		"semi": false,
		"singleQuote": true,
		"endOfLine": "lf",
		"trailingComma": "es5",
		"plugins": [
			"prettier-plugin-svelte"
		],
		"useTabs": true,
		"overrides": [
			{
				"files": "*.svelte",
				"options": {
					"parser": "svelte"
				}
			}
		]
	}
}
